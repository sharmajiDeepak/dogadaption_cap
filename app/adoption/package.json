    "build-local": "ui5 build preload --clean-dest",
    "start": "ui5 serve"
  },
  "ui5": {
    "dependencies": [
      "ui5-task-zipper"
    ]
  },
  "dependencies": {
    "dogadaption": "file:../.."
  }
