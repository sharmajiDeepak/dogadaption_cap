{
  "name": "adoption",
  "version": "0.0.1",
  "description": "An SAP Fiori application.",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "devDependencies": {
    "@sap/ux-specification": "^1.124.12",
    "@sap/ux-ui5-tooling": "1",
    "@ui5/cli": "^3.0.0",
    "ui5-task-zipper": "^3"
  },
  "scripts": {
